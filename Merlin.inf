;
; inf file for installing the Merlin.acs file
;

[Version]
Signature=$Chicago$

[DestinationDirs]
CharacterCopy = 25,msagent\chars

[DefaultInstall]
CheckAdminRights = 1
CopyFiles = CharacterCopy

[DefaultInstall.NT]
CheckAdminRights = 1
CopyFiles = CharacterCopy

;
; Files section for the install

[CharacterCopy]
Merlin.acs

[SourceDisksNames]
1 = %DiskName%,Character,0

[SourceDisksFiles]
